        value     0.0 }
    case otherwise {
        condition always
        value     ( D_MDOTA_91_PA[SJR_PULSE] * EC_MDOTA_DV(-1) 
                  + GP_91_PA[SJR_PULSE] * EC_GP_91)
                  / ( R_91_PA_FSL012[SJR_PULSE] ) }
}

