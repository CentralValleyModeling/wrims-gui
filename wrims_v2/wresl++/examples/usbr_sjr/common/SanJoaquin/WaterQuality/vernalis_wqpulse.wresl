                  + GP_91_PR[SJR_PULSE] * EC_GP_91)
                  / ( R_91_PR_FSL005[SJR_PULSE] ) }
}

define EC_FSL005 {
    case NearZero {
        condition C_FSL005[SJR_PULSE] - min(0.,SG_FSL005_886[SJR_PULSE]) < .1
        value 0.0}
    case otherwise {
        condition always
        value (I_FSL012 * EC_FSL012
             + I_JBP006 * EC_JBP006
             + min(0.,SG_FSL012_892[SJR_PULSE]) * EC_FSL012
             + max(0.,SG_FSL012_892[SJR_PULSE]) * EC_SG_FSL012
             + max(0.,SG_FSL005_886[SJR_PULSE]) * EC_SG_FSL005
             + R_91_PR_FSL005[SJR_PULSE]        * EC_R_91_PR   ! EC_MP_RF_Return 
             + R_91_PA_FSL012[SJR_PULSE]        * EC_R_91_PA ) ! EC_MP_Ag_Return)
             / ( C_FSL005[SJR_PULSE] - min(0.,SG_FSL005_886[SJR_PULSE]) ) }
}

define EC_MDOTA {  ! Mendota Pool
    case NearZero {
        condition C_MDOTA[SJR_PULSE]          + D_MDOTA_91_PA[SJR_PULSE] + D_MDOTA_91_PR[SJR_PULSE]
                  + D_MDOTA_90_PA1[SJR_PULSE] + D_MDOTA_73_XA[SJR_PULSE] + D_MDOTA_XCC010[SJR_PULSE]
                  + D_MDOTA_64_XA[SJR_PULSE]  - min(0.,SG_MDOTA_872[SJR_PULSE]) < 0.001
        value     0.0 }
    case otherwise {
        condition always
        value     ( C_SJR205[SJR_PULSE]             * EC_SJR205
                  + C_FSL005[SJR_PULSE]             * EC_FSL005
                  + C_DMC116[SJR_PULSE]             * EC_DMC070
                  + max(0.,SG_MDOTA_872[SJR_PULSE]) * EC_SG_MDOTA )
                  / ( C_MDOTA[SJR_PULSE]        + D_MDOTA_91_PA[SJR_PULSE] + D_MDOTA_91_PR[SJR_PULSE]
                    + D_MDOTA_90_PA1[SJR_PULSE] + D_MDOTA_73_XA[SJR_PULSE] + D_MDOTA_XCC010[SJR_PULSE]
                    + D_MDOTA_64_XA[SJR_PULSE]  - min(0.,SG_MDOTA_872[SJR_PULSE]) ) }
}
goal setEC_MDOTA_DV {EC_MDOTA_DV = EC_MDOTA}

