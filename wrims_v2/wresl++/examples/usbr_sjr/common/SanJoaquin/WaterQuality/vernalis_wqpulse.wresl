! COMPUTE GOODWIN FLOW REQUIREMENTS TO MEET STANDARD
define Goodwin_Req_pulse1 { value R_RFS61B_STS036[SJR_PULSE]*(VernWQstd - EC_R_61_PA2)/(EC_Goodwin - VernWQstd)
                              + R_RFS61C_STS030[SJR_PULSE]*(VernWQstd - EC_R_61_PA2)/(EC_Goodwin - VernWQstd)
                              - D_STS030_61_NA4[SJR_PULSE]*(VernWQstd - EC_STS030)/(EC_Goodwin - VernWQstd)
                              + R_RFS61D_STS030[SJR_PULSE]*(VernWQstd - EC_R_61_NA2)/(EC_Goodwin - VernWQstd)
                              + SR_61_STS017*(VernWQstd - EC_SR_61_STS017)/(EC_Goodwin - VernWQstd)
                              }

define Goodwin_Req_pulse2 { value C_SJR075[SJR_PULSE]*(VernWQstd - EC_SJR075)/(EC_Goodwin - VernWQstd)
                              + R_71_NA2_SJR070[SJR_PULSE]*(VernWQstd - EC_R_71_NA2)/(EC_Goodwin - VernWQstd)
                              + R_RFS61A_SJR072[SJR_PULSE]*(VernWQstd - EC_R_RFS61A)/(EC_Goodwin - VernWQstd)
                              + R_61_NA4_SJR072[SJR_PULSE]*(VernWQstd - EC_R_61_NA4)/(EC_Goodwin - VernWQstd)
                              + SR_71_SJR070*(VernWQstd - EC_SR_71_SJR070)/(EC_Goodwin - VernWQstd)
                              + SR_61_SJR070*(VernWQstd - EC_SR_61_SJR070)/(EC_Goodwin - VernWQstd)
                              }

define Goodwin_Req_pulse {value Goodwin_Req_pulse1 + Goodwin_Req_pulse2}

define WQ_pulse_def {
    case AprMay {
        condition   month >= apr .and. month <= may
        value       max(0.,Goodwin_Req_pulse-C_STS059[SJR_PULSE]) }
    case otherwise {
        condition   always
        value       0. }
        }

define WQ_reduced_pulse_def {
    case Apr {                                                                                                   !WB
        condition   month == apr                                                                                 !WB
        value       max (0., min ( (remWQrelCap(-1) * TAF_cfs - C_MELONwq[SJR_WQ1]*14./30.), WQ_pulse_def )) }   !WB
    case may {                                                                                                   !WB
        condition   month == may                                                                                 !WB
        value       max (0., min ( (remWQrelCap(-1) * TAF_cfs - C_MELONwq[SJR_WQ1]*15./31.), WQ_pulse_def )) }   !WB
     case otherwise {
          condition always
          value          0. }
        }

goal capC_MELONwqcycle4 { C_MELONWQ < WQ_reduced_pulse_def }
! goal maintainC_MELONm1cycle4 { C_MELONm > max(0.,C_MELONm[SJR_PULSE] - .001) }
! goal maintainC_MELONm2cycle4 { C_MELONm < C_MELONm[SJR_PULSE] + .001 }
goal maintainC_MELONm2cycle4 { C_MELONm = C_MELONm[SJR_PULSE]  }


