!WRESL code for Vernalis Water Quality under Pulse Period Flow Conditions
! VERNALIS_WQPULSE
! Edward Chang
! 5/01/2000
! Modified by Ken Yokoyama,USBR, April 15,2003 for Link-Node method
!
!The following code calculates the required flows below Goodwin Dam to meet the Vernalis
!water quality requirement during the pulse period (April 15 - May 15)
!*****************************************************************************************
!The Vernalis water quality requirement is an Electrical Conductivity (EC) requirement of
!700 and 1000 micromhos/cm for the irrigation (Apr-Aug) and non-irragation (Sep-Mar) seasons,
!respectively.  This is different from the irrigation and non-irrigation season used to label
!the salinities of Goodwin, Accretions, main-stem, and west-side return flows (which use an
!irrigation season of March thru September.  The EC of the flows without Goodwin Dam flows
!are calculated by combining the flows and salinity from the Stanislaus river accretions and
!returns, main-stem flows of the San Joaquin river before confluence with the Stanislaus river,
!and the west-side return flows.  Based on these flows and salinities, and a known salinity for
!Goodwin flows, a flow requirement is calculated at Goodwin to ensure that the water quality
!requirement at Vernalis is met.
!
!******************************************************************************************
!Cycle 4 (Water Quality for pulse periods)   takes into account FERC flows on Tuolumne River
! *********************************************************************************************************************************
!
! This is the link-node salinity algorithm covering the San Joaquin River from Lander Ave. to Vernalis.  The link-node algorithm
! consists of a series of EC mass balance equations from node 614 to node 639.  The computed EC from an upstream node is used as
! the input EC of a downstream node.  Flow-EC regressions are used for the San Joaquin River at Lander Ave., Merced River near
! Stevinson and the Tuolumne River near Modesto. Mud and Salt Slough, return Flow and accretion EC use monthly average values.
! Written by Ken Yokoyama and Nancy Parker, April 2003

/*Tributary Boundary Conditions to the link-node domain
EC[microS/cm] = f(flow[cfs]), subject to max/min constraints
(i.e. Regression Equations defining EC as a function of flow for the following tribs:
-- San Joaquin River at Lander Ave.
-- Merced River near Stevinson
-- Tuolumne River near Modesto)
Equation parameters developed by D. Steiner - 7/29/04, reflecting ~WY2000-2004 observations
Max/min constraints on EC at each boundary were proposed by D. Steiner based on the same
recent observations.
*/
define Mer_WQparam_1 {value -100.52}
define Mer_WQparam_2 {value 729.58}
define Tuo_WQparam_1 {value -57.018}
define Tuo_WQparam_2 {value 500.75}

define EC_MCD002 {value min(500., max(85., Mer_WQparam_1*log(max(1.,C_MCD002[SJR_PULSE])) + Mer_WQparam_2))}! Merced at Stevinson
define EC_TUO003 {value min(275., max(85., Tuo_WQparam_1*log(max(1.,C_TUO003[SJR_PULSE])) + Tuo_WQparam_2))}!Tuolumne at Modesto

! ORIGINAL FORMULATION
/*
! Assume that D_DMC070_CAA069 all goes to San Luis, and the balance of diversion to
! San Luis is from the CAA.  This allows to distinguish the salinity of the sources
! of inflow to San Luis.
! NOTE: THIS ASSUMPTION IS INVALID FOR MUCH OF THE YEAR...
!       DIFFERENCES BETWEEN D_DMC070_CAA069 AND D_CAA069_SLUIS_F ARE GREATER THAN +/- 1000CFS FOR MUCH OF THE YEAR!
define EC_SLUIS_dv  {std kind 'salinity' units 'mS/cm'}
define EC_SLUIS     {value ( S_SLUIS(-1)*taf_cfs * EC_SLUIS_dv(-1)
                           + D_DMC070_CAA069[SJR_PULSE] * EC_Tracy
                           + max(0.,D_CAA069_SLUIS[SJR_PULSE]-D_DMC070_CAA069[SJR_PULSE]) * EC_Banks
                           + I_SLUIS * EC_ISLUIS
                           - (C_SLUIS[SJR_PULSE] + D_SLUIS_SFLPE[SJR_PULSE]) * EC_SLUIS_dv(-1) )
                           / ( S_SLUIS[SJR_PULSE]*taf_cfs ) }                    
goal setEC_SLUIS_dv {EC_SLUIS_dv = EC_SLUIS}
 
define EC_CAA069    {value (C_CAA067[SJR_PULSE] * EC_Banks
                           + C_SLUIS[SJR_PULSE] * EC_SLUIS
                           + D_DMC070_CAA069[SJR_PULSE] * EC_Tracy)  
                           / (C_CAA069[SJR_PULSE] + D_CAA069_DMC070[SJR_PULSE])}

define EC_DMC070    {value (C_DMC064[SJR_PULSE] * EC_Tracy
                       !   + D_CAA069_DMC070[SJR_PULSE] * EC_CAA069
                           + C_SLUIS_CVP[SJR_PULSE] * EC_SLUIS            ! does this term need to be here? Isn't this double counting SLUIS outflows to DMC???
                           + max(0.,D_CAA069_DMC070[SJR_PULSE]-C_SLUIS_CVP[SJR_PULSE]) * EC_Banks)  
                           / (D_DMC070_CAA069[SJR_PULSE] + C_DMC070[SJR_PULSE] 
                           + D_DMC070_VLW008[SJR_PULSE] + D_DMC070_73_PA1[SJR_PULSE])}
*/


! ALTERNATIVE SLUIS EC BALANCE -- ASSUME ALL CVP INFLOW FROM TRACY, SWP FROM BANKS
/*
! NOTE: THIS ASSUMPTION MAY BE INVALID IF WHEELING IS SIGNIFICANT
! NOTE: BALANCE AT CAA069 DOES NOT ACCOUNT FOR INTERTIE CONTRIBUTION TO C_CAA067
define EC_SLUIS_dv  {std kind 'salinity' units 'ms/cm'}
define EC_SLUIS     {value ( S_SLUIS(-1)*taf_cfs * EC_SLUIS_dv(-1)
                           + D_CAA069_SLUIS_F[SJR_PULSE] * EC_Tracy
                           + D_CAA069_SLUIS_S[SJR_PULSE] * EC_Banks
                           + I_SLUIS * EC_ISLUIS
                           - C_SLUIS[SJR_PULSE] * EC_SLUIS_dv(-1)
                           - D_SLUIS_SFLPE[SJR_PULSE] * EC_SLUIS_dv(-1) )
                           / ( S_SLUIS[SJR_PULSE]*taf_cfs ) }
goal setEC_SLUIS_dv {EC_SLUIS_dv = EC_SLUIS}

! Same as original
define EC_CAA069    {value ( C_CAA067[SJR_PULSE] * EC_Banks
                           + C_SLUIS[SJR_PULSE]  * EC_SLUIS
                           + D_DMC070_CAA069[SJR_PULSE] * EC_Tracy)
                           / ( C_CAA069[SJR_PULSE] + D_CAA069_DMC070[SJR_PULSE] ) }

! Updated from original...don't double-count SLUIS inflows, include SG terms
define EC_DMC070    {value ( C_DMC064[SJR_PULSE] * EC_Tracy
                           + D_CAA069_DMC070[SJR_PULSE] * EC_CAA069
                           + max(0.0,SG_DMC070_758[SJR_PULSE]) * EC_SG_DMC070
                           + max(0.0,SG_DMC070_763[SJR_PULSE]) * EC_SG_DMC070 )
                           / ( D_DMC070_CAA069[SJR_PULSE] + D_DMC070_VLW008[SJR_PULSE]
                             + D_DMC070_73_PA1[SJR_PULSE] + C_DMC070[SJR_PULSE] 
                             - min(0.0,SG_DMC070_758[SJR_PULSE])
                             - min(0.0,SG_DMC070_763[SJR_PULSE]) ) }
*/                           
                           
                           
! ALTERNATIVE SLUIS EC BALANCE -- ASSUME WHEELING GOES TO SLUIS FIRST, THEN FROM DMC
! NOTE: NEED TO ASSUME EC FOR D_DMC070_CAA069 *OR* D_CAA069_DMC070 BECAUSE CAN'T COMPUTE SIMULTANEOUSLY
!       THIS SETUP ASSUMES EC FOR D_DMC070_CAA069 IS EC_TRACY 
!       (i.e., assumes diversion to CAA069 before mixing at node DMC070)
!       (this is accounted for in DMC070 balance by setting inflow from DMC as C_DMC064-D_DMC070_CAA069)

define EC_SLUIS_dv  {std kind 'salinity' units 'mS/cm'}                             

define EC_CAA009    {value ( C_CAA003[SJR_PULSE]               * EC_Banks
                           + D_DMC007_CAA009[SJR_PULSE]        * EC_Tracy )
                           / ( C_CAA009[SJR_PULSE] ) }

define EC_CAA067    {value EC_CAA009}

define EC_CAA069    {value ( C_CAA067[SJR_PULSE]               * EC_CAA067
                           + D_DMC070_CAA069[SJR_PULSE]        * EC_Tracy            ! necessary to assume EC_DMC070 = EC_Tracy...
                           + C_SLUIS[SJR_PULSE]                * EC_SLUIS_dv(-1) )   ! necessary to assume EC_SLUIS @ previous time...
                           / (C_CAA069[SJR_PULSE] + D_CAA069_DMC070[SJR_PULSE] + D_CAA069_SLUIS[SJR_PULSE] ) }

define EC_DMC070    {value ( (C_DMC064[SJR_PULSE] - D_DMC070_CAA069[SJRBASE]) * EC_Tracy
                           + D_CAA069_DMC070[SJR_PULSE]        * EC_CAA069
                           + max(0.0,SG_DMC070_758[SJR_PULSE]) * EC_SG_DMC070        ! seepage gains
                           + max(0.0,SG_DMC070_763[SJR_PULSE]) * EC_SG_DMC070 )      ! seepage gains
                           / ( D_DMC070_CAA069[SJR_PULSE] + D_DMC070_VLW008[SJR_PULSE] 
                             + D_DMC070_73_PA1[SJR_PULSE] + C_DMC070[SJR_PULSE]
                             - min(0.0,SG_DMC070_758[SJR_PULSE])                     ! seepage losses
                             - min(0.0,SG_DMC070_763[SJR_PULSE]) ) }                 ! seepage losses
                             
define EC_SLUIS     {value ( S_SLUIS(-1)*taf_cfs               * EC_SLUIS_dv(-1)
                           + D_CAA069_SLUIS[SJR_PULSE]         * EC_CAA069
                           + I_SLUIS                           * EC_ISLUIS
                           - C_SLUIS[SJR_PULSE]                * EC_SLUIS_dv(-1)
                           - D_SLUIS_SFLPE[SJR_PULSE]          * EC_SLUIS_dv(-1) )
                           / ( S_SLUIS[SJR_PULSE]*taf_cfs ) }
goal setEC_SLUIS_dv {EC_SLUIS_dv = EC_SLUIS}


