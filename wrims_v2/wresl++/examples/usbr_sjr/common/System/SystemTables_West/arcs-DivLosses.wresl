define EV_MDOTA_91_PA     {std kind 'EVAPORATION-LOSS'  units 'CFS'}
define DP_MDOTA_91_PA     {std kind 'DEEP-PERCOLATION'  units 'CFS'}
define LF_MDOTA_91_PA     {std kind 'LATERAL-FLOW-LOSS' units 'CFS'}
define OS_MDOTA_91_PA     {std kind 'SPILL-LOSS'        units 'CFS'}
define DL_MDOTA_91_PA     {std kind 'DELIVERY-LOSS'     units 'CFS'}
define EVF_MDOTA_91_PA    {value 0.00} ! evaporative loss fraction
define DPF_MDOTA_91_PA    {value 0.00} ! deep percolation loss fraction
define LFF_MDOTA_91_PA    {value 0.00} ! lateral flow loss fraction
define OSF_MDOTA_91_PA    {value 0.00} ! canal operating spill fraction

define TWFO_91_PA         {value RfactorPA}   ! tailwater (non-rice) (fraction of AW)
define TWFR_91_PA         {value RfactorPA}   ! tailwater (rice)     (fraction of AW)
define RUFR_91_PA         {value 0.0*RUFRmax} ! reuse fraction for irrigated rice
define RUFO_91_PA         {value 0.00}        ! reuse fraction for other irrigated crops
define RUFW_91_PA         {value 0.00}        ! reuse fraction for wetlands 
define RPF_91_PA          {value 0.00}        ! riparian/miscellaneous ET fraction 

define EV_91_PA           {std kind 'EVAPORATIVE-LOSS'  units 'CFS'}
define DP_91_PA           {std kind 'DEEP-PERCOLATION'  units 'CFS'}
define LF_91_PA           {std kind 'LATERAL-FLOW-LOSS' units 'CFS'}
define OS_91_PA           {std kind 'SPILL-LOSS'        units 'CFS'}
define DL_91_PA           {std kind 'DELIVERY-LOSS'     units 'CFS'}

define EVF_MDOTA_91_PA_a  {alias EVF_MDOTA_91_PA kind 'loss-factor' units 'none'}


