component wrims_core as "WRIMS-Core" {
    package main #orange
    package serial #orange
    package wrimsv2 {
        package commondata {
            package solverdata
            package tabledata
            package wresldata
        }
        package components
        package config
        package debug
        package evaluator
        package exception #pink
        package external
        package hdf5
        package ilp
        package launch
        package parallel
        package solver {
            package cbc
